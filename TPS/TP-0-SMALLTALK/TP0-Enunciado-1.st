Object subclass: #ComisionCurricularPermanente	instanceVariableNames: 'pilaExpendientes expedienteTratados'	classVariableNames: ''	package: 'TP0-Enunciado-1'!!ComisionCurricularPermanente methodsFor: 'initialization' stamp: 'Ezequiel 10/10/2020 18:17'!initialize	pilaExpendientes := OrderedCollection new.	expedienteTratados := 0.! !!ComisionCurricularPermanente methodsFor: 'as yet unclassified' stamp: 'Ezequiel 10/10/2020 18:29'!votarEnContra.	(pilaExpendientes at: (expedienteTratados + 1)) sumarVotoNegativo.! !!ComisionCurricularPermanente methodsFor: 'as yet unclassified' stamp: 'Ezequiel 10/10/2020 18:26'!procesarExpediente	|elemento|	elemento := pilaExpendientes at:(expedienteTratados  + 1).	expedienteTratados  := expedienteTratados + 1.	(elemento votosAFavor > 1) ifFalse: [		elemento rechazar.	] ifTrue: [ 		elemento aprobar.	].	^ elemento.! !!ComisionCurricularPermanente methodsFor: 'as yet unclassified' stamp: 'Ezequiel 10/10/2020 17:59'!recibirExpediente: unExpediente.	pilaExpendientes add: unExpediente.! !!ComisionCurricularPermanente methodsFor: 'as yet unclassified' stamp: 'Ezequiel 10/10/2020 18:30'!votarAFavor.	(pilaExpendientes at: (expedienteTratados + 1)) sumarVotoPositivo.! !Object subclass: #Expediente	instanceVariableNames: 'nombre estado votosAfavor votosEncontra'	classVariableNames: ''	package: 'TP0-Enunciado-1'!!Expediente methodsFor: 'accessing' stamp: 'Ezequiel 10/10/2020 17:53'!estado.	^ estado.! !!Expediente methodsFor: 'as yet unclassified' stamp: 'Ezequiel 10/10/2020 18:24'!sumarVotoNegativo.	votosEncontra  := votosEncontra + 1.! !!Expediente methodsFor: 'as yet unclassified' stamp: 'Ezequiel 10/10/2020 18:13'!votosEnContra.	^ votosEncontra.! !!Expediente methodsFor: 'as yet unclassified' stamp: 'Ezequiel 10/10/2020 18:13'!votosAFavor.	^ votosAfavor.! !!Expediente methodsFor: 'as yet unclassified' stamp: 'Ezequiel 10/10/2020 17:53'!establecerNombre: unNombre.	nombre := unNombre.! !!Expediente methodsFor: 'as yet unclassified' stamp: 'Ezequiel 10/10/2020 18:24'!sumarVotoPositivo.	votosAfavor := votosAfavor + 1.! !!Expediente methodsFor: 'as yet unclassified' stamp: 'Ezequiel 10/10/2020 18:22'!rechazar.	estado := 'Rechazado'.! !!Expediente methodsFor: 'as yet unclassified' stamp: 'Ezequiel 10/10/2020 18:23'!aprobar	estado := 'Aprobado'.! !!Expediente methodsFor: 'initialization' stamp: 'Ezequiel 10/10/2020 17:58'!initialize 	nombre := ''.	estado := 'Iniciado'.	votosAfavor := 0.	votosEncontra := 0.! !